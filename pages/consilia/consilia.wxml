<view class="index">
  <view class="page-indicator">
    <view>就诊日期：</view>
    <view>{{consilia.visitingDate}}</view>
  </view>
  <view class="index-bd">
    <view class="consilia-basic-info-hd">
      <view class="consilia-basic-info">{{consilia.name}}</view>
      <view class="consilia-basic-info">{{consilia.sex}}</view>
      <view class="consilia-basic-info">{{consilia.age}}</view>
    </view>
    <view class="consilia-basic-info-hd">
      <view class="consilia-basic-info">{{consilia.birthday}}</view>
      <view class="consilia-basic-info">{{consilia.zodiac}}</view>
    </view>
    <view class="consilia-pulse-info-hd">
      <view class="consilia-pulse-const">脉象：</view>
      <view class="consilia-pulse-var">{{consilia.pulse}}</view>
    </view>
    <view class="consilia-tongue-info-hd">
      <view class="consilia-tongue-const">舌象：</view>
      <view class="consilia-tongue-var">{{consilia.tongue}}</view>
    </view>
    <view class="consilia-medicine-name-info-hd">
      <view class="consilia-medicine-name-const">方：</view>
      <view class="">{{consilia.prescriptionName}}</view>
    </view>
    <view class="consilia-medicine-detail-info-hd">
      <view class="consilia-medicine-detail-const">药：</view>
      <view class="">{{consilia.prescriptionDetail}}</view>
    </view>
    <view class="consilia-medicine-dosage-info-hd">
      <view class="consilia-medicine-dosage-const">剂量：</view>
      <view class="">{{consilia.prescriptionCount}}</view>
      <view class="">剂/</view>
      <view class="">{{consilia.prescriptionDuration}}</view>
    </view>
    <view class="consilia-medicine-method-info-hd">
      <view class="consilia-medicine-method-const">煎服方法：</view>
      <view class="">{{consilia.prescriptionMethod}}</view>
    </view>
    <view class="consilia-zhaoSir-say-info-hd">
      <view class="consilia-zhaoSir-say-const">师曰：</view>
    </view>

  </view>
</view>