<view class="index">
  <view class="page-indicator">
    <view>{{visitingDate}}</view>
  </view>
  <view class="index-bd">
    <view class="kind-list">
      <block wx:for-items="{{patientIntroList}}" wx:key="{{item.patientInfoId}}">
        <navigator url="">
          <view class="common-list-item">
            <view id="{{item.patientInfoId}}" class="common-list-item-hd">
              <view class="common-list-text">姓名：</view>
              <view class="common-list-text">{{item.patientName}}</view>
            </view>
            <view id="{{item.patientInfoId}}" class="common-list-item-hd">
              <view class="common-list-text">性别：</view>
              <view class="common-list-text">{{item.patientSex}}</view>
            </view>    
          </view>
          </navigator>
      </block>
    </view>
  </view>
</view>
